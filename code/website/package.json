{
  "name": "uiharness.website",
  "version": "0.0.2-beta.1",
  "description": "Website for the UIHarness.",
  "types": "lib/index.d.ts",
  "scripts": {
    "ssr": "cd src.server && yarn ssr $@",
    "ui": "ui $@",
    "start": "ui start web $@",
    "watch": "cd src.server && yarn watch $@",
    "test": "ts test",
    "tdd": "ts test --watch",
    "lint": "ts lint",
    "build": "ts build --no-esm $@",
    "build-test": "ts build --tsconfig=tsconfig.test $@",
    "prepare": "ts prepare",
    "bundle": "ui bundle web",
    "deploy": "cd src.server && yarn deploy"
  },
  "dependencies": {
    "@platform/react": "0.3.24",
    "@platform/ui.icon": "0.0.88",
    "@platform/ui.shell": "0.1.16"
  },
  "devDependencies": {
    "@platform/test": "0.0.63",
    "@platform/ts": "3.5.4",
    "@uiharness/dev": "0.9.3",
    "@uiharness/ui": "0.6.14"
  },
  "files": [
    "lib",
    "!*.TEST.{js, mjs, d.ts}"
  ],
  "license": "MIT",
  "private": true
}
