{
  "name": "@uiharness/ui",
  "version": "0.0.53",
  "scripts": {
    "postinstall": "ui init",
    "ui": "yarn chmod && ui $@",
    "start": "yarn chmod && ui start $@",
    "test": "yarn chmod && ts test",
    "tdd": "yarn chmod && ts test --watch",
    "lint": "yarn chmod && ts lint",
    "build": "yarn chmod && ts build",
    "prepare": "yarn chmod && ts prepare",
    "chmod": "chmod 777 node_modules/.bin/ts"
  },
  "dependencies": {
    "@uiharness/electron": "0.3.93"
  },
  "devDependencies": {
    "@platform/test": "0.0.25",
    "@platform/ts.cli": "3.3.24",
    "@uiharness/dev": "0.0.35"
  },
  "resolutions": {
    "@types/react": "16.7.17"
  },
  "files": [
    "lib",
    "src"
  ],
  "engines": {
    "node": ">=10.x"
  },
  "keywords": [
    "design",
    "test",
    "react",
    "typescript",
    "types",
    "ui"
  ],
  "license": "MIT"
}
